{
  "name": "typescript-api",
  "version": "1.0.0",
  "description": "Typescript node.jss API",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "yarn build && node dist/src/index.js",
    "start:dev": "ts-node-dev src/index.ts",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "style:check": "prettier --check src/**/*.ts test/**/*.ts",
    "style:fix": "prettier --write src/**/*.ts test/**/*.ts",
    "test:unit": "jest",
    "test:functional": "jest --projects ./test --runInBand"
  },
  "engines": {
    "node": "16"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LeonardoMonti/node-typescript-api.git"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "api"
  ],
  "author": "Leonardo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/LeonardoMonti/node-typescript-api/issues"
  },
  "homepage": "https://github.com/LeonardoMonti/node-typescript-api#readme",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/config": "^3.3.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.3",
    "@types/module-alias": "^2.0.1",
    "@types/node": "16",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.36.0",
    "@typescript-eslint/parser": "^5.36.0",
    "eslint": "^8.23.0",
    "jest": "^27.4.5",
    "nock": "^13.2.9",
    "path": "^0.12.7",
    "prettier": "^2.7.1",
    "supertest": "^6.2.4",
    "ts-jest": "^27.1.2",
    "typescript": "^4.8.2"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.6",
    "@types/jsonwebtoken": "^8.5.9",
    "axios": "^0.27.2",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.0",
    "config": "^3.3.7",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "mongoose": "^6.5.4"
  }
}
